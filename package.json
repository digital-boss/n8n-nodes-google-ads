{
  "name": "@digital-boss/n8n-nodes-google-ads",
  "version": "1.0.0",
  "author": "IÃ±aki Breinbauer",
  "license": "GPL-3.0",
  "description": "Google Ads support for n8n",
  "keywords": [
    "n8n",
    "nodemation",
    "dnc",
    "digital north consulting",
    "nodes",
    "custom",
    "Google Ads",
    "workflow"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quansenB/n8n-nodes-google-ads.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "tsc && gulp",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "watch": "tsc --watch",
    "test": "jest",
    "release": "npm run build && npm publish --access=public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/GoogleAdsOAuth2Api.credentials.js",
      "dist/credentials/GoogleAdsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Google/Ads/GoogleAds.node.js"
    ]
  },
  "devDependencies": {
    "@types/aws4": "^1.11.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "14.17.27",
    "@types/request": "^2.48.7",
    "@types/request-promise-native": "^1.0.18",
    "gulp": "^4.0.2",
    "jest": "^27.1.0",
    "n8n-workflow": "~0.79.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@types/request": "^2.48.7",
    "@types/request-promise-native": "^1.0.18",
    "aws4": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "n8n-core": "~0.96.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
